<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="250dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">


    <ImageView
        android:id="@+id/imageView_myorder_history"
        android:layout_width="match_parent"
        android:layout_height="184dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.431" />


    <TextView
        android:id="@+id/textView_myorder_name_history"
        android:layout_width="273dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Titulli i produktit"
        android:textColor="@color/myorder_titleproduct_black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history" />

    <TextView
        android:id="@+id/textView_myorder_metric_history"
        android:layout_width="273dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="58dp"
        android:text="250ml"
        android:textColor="@color/myorder_titleproduct_black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history" />

    <TextView

        android:id="@+id/textView_myorder_price_history"
        android:layout_width="273dp"
        android:layout_height="40dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="96dp"
        android:paddingLeft="4dp"
        android:text="cmimi i produktit"
        android:textColor="@color/myorder_titleproduct_black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/circularImageView_myorder_history"
        android:layout_width="127dp"
        android:layout_height="98dp"
        android:layout_marginBottom="116dp"
        android:layout_marginStart="428dp"
        android:src="@drawable/ic_launcher_background"
        app:civ_border_color="#EEEEEE"
        app:civ_border_width="4dp"
        app:civ_shadow_color="#8BC34A"
        app:civ_shadow_radius="10"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_myorder_history"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        tools:ignore="MissingConstraints" />

    <TextView
        android:textSize="18sp"
        android:id="@+id/textview_totalitems_history"
        android:layout_width="273dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="136dp"
        android:background="#0000"
        android:textColor="@color/myorder_titleproduct_black"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history"
        app:srcCompat="@drawable/ic_info" />

    <TextView
        android:textSize="18sp"
        android:id="@+id/textview_status_history"
        android:layout_width="273dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="136dp"
        android:textColor="@color/myorder_titleproduct_black"
        app:layout_constraintEnd_toEndOf="@+id/imageView_myorder_history"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history" />
</android.support.constraint.ConstraintLayout>