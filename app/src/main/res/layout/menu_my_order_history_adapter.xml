<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="190dp">


    <ImageView
        android:id="@+id/imageView_myorder_history"
        android:layout_width="match_parent"
        android:layout_height="144dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.76" />


    <TextView
        android:id="@+id/textView_myorder_name_history"
        android:layout_width="175dp"
        android:layout_height="23dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:text="Titulli i produktit"
        android:textColor="@color/myorder_titleproduct_black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history" />

    <TextView
        android:id="@+id/textView_myorder_price_history"
        android:layout_width="90dp"
        android:layout_height="21dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="56dp"
        android:paddingLeft="4dp"
        android:text="cmimi i produktit"
        android:textColor="@color/myorder_titleproduct_black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/circularImageView_myorder_history"
        android:layout_width="92dp"
        android:layout_height="80dp"
        android:layout_marginBottom="94dp"
        android:layout_marginStart="232dp"
        android:src="@drawable/ic_launcher_background"
        app:civ_border_color="#EEEEEE"
        app:civ_border_width="4dp"
        app:civ_shadow_color="#8BC34A"
        app:civ_shadow_radius="10"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_myorder_history"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textview_totalitems_history"
        android:layout_width="90dp"
        android:layout_height="24dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="106dp"
        android:background="#0000"
        android:textColor="@color/myorder_titleproduct_black"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history"
        app:srcCompat="@drawable/ic_info" />

    <TextView
        android:id="@+id/textview_status_history"
        android:layout_width="120dp"
        android:layout_height="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="284dp"
        android:layout_marginTop="106dp"
        android:textColor="@color/myorder_titleproduct_black"
        app:layout_constraintEnd_toEndOf="@+id/imageView_myorder_history"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history" />
</android.support.constraint.ConstraintLayout>