<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/imageView_myorder_history_dialog"
        android:layout_width="258dp"
        android:layout_height="132dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.357" />


    <TextView
        android:id="@+id/textView_myorder_name_history_dialog"
        android:layout_width="129dp"
        android:layout_height="19dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="16dp"
        android:text="Titulli i produktit"
        android:textColor="@color/myorder_titleproduct_black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history_dialog"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history_dialog" />

    <TextView
        android:id="@+id/textView_myorder_metric_history_dialog"
        android:layout_width="133dp"
        android:layout_height="17dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="58dp"
        android:text="250ml"
        android:textColor="@color/myorder_titleproduct_black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history_dialog"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history_dialog" />

    <TextView
        android:id="@+id/textView_myorder_quantity_history_dialog"
        android:layout_width="88dp"
        android:layout_height="17dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="96dp"
        android:paddingLeft="4dp"
        android:text="cmimi i produktit"
        android:textColor="@color/myorder_titleproduct_black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history_dialog"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history_dialog" />

    <com.mikhaellopez.circularimageview.CircularImageView
        android:id="@+id/circularImageView_myorder_history_dialog"
        android:layout_width="90dp"
        android:layout_height="68dp"
        android:layout_marginBottom="88dp"
        android:layout_marginStart="156dp"
        android:src="@drawable/ic_launcher_background"
        app:civ_border_color="#EEEEEE"
        app:civ_border_width="4dp"
        app:civ_shadow_color="#8BC34A"
        app:civ_shadow_radius="10"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_myorder_history_dialog"
        app:layout_constraintStart_toStartOf="@+id/imageView_myorder_history_dialog"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView_myorder_history_dialog"
        android:layout_width="124dp"
        android:layout_height="21dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="96dp"
        android:text="TextView"
        android:textColor="@color/myorder_titleproduct_black"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/textView_myorder_metric_history_dialog"
        app:layout_constraintTop_toTopOf="@+id/imageView_myorder_history_dialog" />


</android.support.constraint.ConstraintLayout>
